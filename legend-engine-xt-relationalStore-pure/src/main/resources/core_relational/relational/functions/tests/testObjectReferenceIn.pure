Mapping meta::external::store::relational::tests::functions::objectReferenceIn::simpleModelMapping
(
   Person : Pure
            {
               ~src _Person
               firstName : $src.fullName->substring(0, $src.fullName->indexOf(' ')),
                              lastName : $src.fullName ->substring($src.fullName->indexOf(' ')+1, $src.fullName->length()),
               addresses : $src.addresses
            }
   
   Address : Pure
            {
               ~src Address
               street : $src.street
            } 
   
   Firm : Pure
   {
      ~src _Firm
      legalName : $src.name,
      employees : $src.employees
   } 
)
