import meta::external::store::relational::functions::sqlstring::*;

Class meta::external::store::relational::memsql::tests::db::A
{
  id: Integer[1];
  window : Integer[1];
}

function meta::external::store::relational::memsql::tests::db::testReservedKeywords() : Boolean[1]
{
  let result = toSQLString(
    | meta::external::store::relational::memsql::tests::db::A.all()->project([s | $s.window], ['WINDOW']),
      meta::external::store::relational::memsql::tests::db::simpleRelationalMappingForMemSQLWindowKeyword,
      meta::external::store::relational::runtime::DatabaseType.MemSQL,
      meta::pure::router::extension::defaultRelationalExtensions());

  assertEquals('select `root`.`window` as `WINDOW` from `test table` as `root`', $result);
}

###Relational
Database meta::external::store::relational::memsql::tests::db::schema1
(
   Table "test table" (ID INT PRIMARY KEY, window INT)
)

###Mapping
Mapping meta::external::store::relational::memsql::tests::db::simpleRelationalMappingForMemSQLWindowKeyword
(
   meta::external::store::relational::memsql::tests::db::A : Relational
   {
      scope([meta::external::store::relational::memsql::tests::db::schema1])
      (
         id : "test table".ID,
         window : "test table".window

      )
   }
)